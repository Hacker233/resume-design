{
  "name": "maobucv",
  "private": true,
  "version": "1.0.0",
  "description": "A Vue 3 project with various tools and libraries.",
  "scripts": {
    "deploy": "npm install && npm run build",
    "dev": "vite",
    "build:local": "rimraf dist && vite build",
    "build:dev": "rimraf dist && vite build --mode development",
    "generate:prerender-data": "ts-node generate-prerender-data.ts",
    "build:spa": "rimraf dist && vite build --mode spa",
    "build:ssr": "rimraf dist && npm run generate-templates && npm run generate:prerender-data && vite build --mode ssr",
    "generate-templates": "node handleTemplateJson.js",
    "preview:dev": "npm run build:dev && vite preview",
    "preview": "npm run build && vite preview",
    "preview:dist": "vite preview",
    "eslint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "prettier": "prettier . --write",
    "commit": "git-cz",
    "postinstall": "husky install",
    "fix-memory-limit": "cross-env LIMIT=4096 increase-memory-limit"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
